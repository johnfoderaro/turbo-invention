{
  "name": "turbo-invention",
  "version": "1.3.0",
  "description": "Basic React App Boiler Plate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack -p --config webpack.prod.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "!node_modules/react-runtime"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnfoderaro/turbo-invention.git"
  },
  "author": "John Foderaro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnfoderaro/turbo-invention/issues"
  },
  "homepage": "https://github.com/johnfoderaro/turbo-invention#readme",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "clean-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-styled-components": "^6.3.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "styled-components": "^4.4.1",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.4.0",
    "react-test-renderer": "^16.13.1",
    "webpack-dev-server": "^3.11.0"
  }
}
